citHeader("To cite package 'soilspec' in publications use:")
     
year <- 2020
vers <- paste("R package version", meta$Version)
     
citEntry(entry="Manual",
  title = "soilspec: functions and data for the book Soil Spectral Inference with R",
  author = personList(as.person("Alexandre MJC Wadoux")),
  publication = "R package",
  year = year,
  note = vers,

  textVersion =
  paste("Alexandre MJC Wadoux (",
        year,
        "). soilspec: functions and data for the book Soil Spectral Inference with R",
        vers, ".", sep=""))
